{
  "name":"Enttec LED Mapper",
  "type":"OSC",
  "path":"Software",
  
  "version":"0.1.0",
  "description":"A Chataigne module to control Enttec LED Mapper ELM via OSC",
  "url":"https://github.com/yastefan/Enttec-LED-Mapper-Chataigne-Module",
  "downloadURL":"https://github.com/yastefan/Enttec-LED-Mapper-Chataigne-Module/archive/refs/heads/main.zip",

  "hasInput":false,
  "hasOutput":true, 

  "hideDefaultCommands":false,
  
  "defaults":
  {	
    "autoAdd":false,
    "oscInput": {"localPort":9001}
  },

  "parameters":
  {

  },

  "hideDefaultParameters": ["autoAdd", "colorSendMode", "useHierarchy", "autoFeedback"],
  "scripts": ["elm.js"],

  "values":
  {
  },

  "commands":
  {
   
   "Change Intensity":
    {
      "menu":"",  
      "callback":"changeIntensity",
      "parameters":
      {
        "Stage": {"type":"String", "default":"Stage01"},
        "Intensity": {"type": "Float", "min":0, "max":1, "default":0, "ui":"slider", "useForMapping":true}
      }
    },
    "Change Speed":
    {
      "menu":"",  
      "callback":"changeSpeed",
      "parameters":
      {
        "Stage": {"type":"String", "default":"Stage01"},
        "Speed": {"type": "Float", "min":0, "max":10, "default":1, "ui":"slider", "useForMapping":true}
      }
    },
    "Change Color":
    {
      "menu":"",  
      "callback":"changeColor",
      "parameters":
      {
        "Stage": {"type":"String", "default":"Stage01"},
        "Color": {"type": "Color", "default": [1,0,0,1], "useForMapping":true}
      }
    },
    "Change Media":
    {
      "menu":"",  
      "callback":"changeMedia",
      "parameters":
      {
        "Stage": {"type":"String", "default":"Stage01"},
        "Media": {"type": "Integer", "min":0, "max":255, "default":0, "useForMapping":true}
      }
    }
  }
}